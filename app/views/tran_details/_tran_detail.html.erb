<div id="<%= dom_id tran_detail %>">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">S.L</th>
        <th scope="col">Transection Details</th>
        <th scope="col">Amount</th>
      </tr>
    </thead>
    <tbody>
      <% @tran_summary.each do |tran|%>
        <tr>
          <td scope="row"></td>
          <td scope="row"><%= tran.itemDetails %></td>
          <td scope="row"><%= tran.amount %></td>
        </tr>
      <%end%>
    </tbody>
  </table>
</div>
