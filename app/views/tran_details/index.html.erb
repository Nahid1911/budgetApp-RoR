<p style="color: green"><%= notice %></p>
<h1>Tran details</h1>
<div id="tran_details">
  <% @tran_details.each do |tran_detail| %>
    <%= render tran_detail %>
    <p>
      <%= link_to "Show this tran detail", tran_detail %>
    </p>
  <% end %>
</div>
<%= link_to "Add New Transaction", new_tran_detail_path, class: "btn btn-outline-success" %>
<%= link_to "Back Index", tran_groups_path, class: "btn btn-outline-warning" %>
